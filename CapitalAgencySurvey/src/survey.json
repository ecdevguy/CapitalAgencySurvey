{
  "pages": [
    {
      "name": "homeInsurancePage1",
      "elements": [
        {
          "type": "boolean",
          "name": "homeInsurance",
          "title": "Interested in Home insurance?",
          "formattedTitle": "Interested in Home insurance?",
          "defaultValue": "true",
          "isRequired": true
        },
        {
          "type": "paneldynamic",
          "name": "homeOwnerInfo",
          "visibleIf": "{homeInsurance} = true",
          "title": "Owner Information (include spouses)",
          "formattedTitle": "Home Owner",
          "templateElements": [
            {
              "type": "text",
              "name": "firstName",
              "title": "First Name",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "lastName",
              "startWithNewLine": false,
              "title": "Last Name",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "birthDate",
              "startWithNewLine": false,
              "title": "Date of birth",
              "isRequired": true,
              "inputType": "date",
              "min": "1900-01-01"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 5,
          "confirmDelete": true,
          "confirmDeleteText": "Are you sure you want to delete this owner?",
          "panelAddText": "Add owner",
          "panelRemoveText": "Remove owner"
        },
        {
          "type": "panel",
          "name": "homeOwnerAddress",
          "visibleIf": "{homeInsurance} = true",
          "title": "Address of Home",
          "formattedTitle": "Home Address",
          "isRequired": true,
          "elements": [
            {
              "type": "text",
              "name": "addressLine1",
              "title": "Address",
              "formattedTitle": "Address Line One",
              "isRequired": true,
              "placeholder": "495 Grove Street"
            },
            {
              "type": "text",
              "name": "addressLine2",
              "title": "Address line 2",
              "formattedTitle": "Address Line Two",
              "placeholder": "Apartment #20"
            },
            {
              "type": "text",
              "name": "city",
              "title": "City/Town",
              "formattedTitle": "City/Town",
              "isRequired": true,
              "placeholder": "New York"
            },
            {
              "type": "text",
              "name": "state",
              "startWithNewLine": false,
              "title": "State/Region/Province",
              "formattedTitle": "State/Region/Province",
              "isRequired": true,
              "placeholder": "NY"
            },
            {
              "type": "text",
              "name": "postCode",
              "title": "Zip/Postal code",
              "formattedTitle": "Zip/Postal Code",
              "isRequired": true,
              "inputType": "number",
              "min": 0,
              "max": 99999,
              "placeholder": "10014"
            },
            {
              "type": "dropdown",
              "name": "country",
              "title": "Country",
              "formattedTitle": "Country",
              "choicesByUrl": {
                "url": "https://surveyjs.io/api/CountriesExample"
              },
              "placeholder": "United States"
            },
            {
              "type": "boolean",
              "name": "isCurrentAddress",
              "title": "This is my current address.",
              "formattedTitle": "Current address",
              "isRequired": true
            }
          ]
        },
        {
          "type": "panel",
          "name": "currentAddress",
          "visibleIf": "{homeInsurance} = true and {isCurrentAddress} = false",
          "title": "Current Address",
          "elements": [
            {
              "type": "text",
              "name": "currentAddressLine1",
              "title": "Address",
              "formattedTitle": "Current Address Line One",
              "isRequired": true,
              "placeholder": "495 Grove Street"
            },
            {
              "type": "text",
              "name": "currentAddressLine2",
              "title": "Address line 2",
              "formattedTitle": "Current Address Line Two",
              "placeholder": "Apartment #20"
            },
            {
              "type": "text",
              "name": "currentCity",
              "title": "City/Town",
              "formattedTitle": "Current City/Town",
              "isRequired": true,
              "placeholder": "New York"
            },
            {
              "type": "text",
              "name": "currentState",
              "startWithNewLine": false,
              "title": "State/Region/Province",
              "formattedTitle": "Current State/Region/Province",
              "isRequired": true,
              "placeholder": "NY"
            },
            {
              "type": "text",
              "name": "currentPostCode",
              "title": "Zip/Postal code",
              "formattedTitle": "Current Zip/Postal Code",
              "isRequired": true,
              "inputType": "number",
              "min": 0,
              "max": 99999,
              "placeholder": "10014"
            },
            {
              "type": "dropdown",
              "name": "currentCountry",
              "title": "Country",
              "formattedTitle": "Current Country",
              "choicesByUrl": {
                "url": "https://surveyjs.io/api/CountriesExample"
              },
              "placeholder": "United States"
            }
          ]
        },
        {
          "type": "panel",
          "name": "basementInfo",
          "title": "Basement Information",
          "visibleIf": "{homeInsurance} = true",
          "formattedTitle": "Basement",
          "elements": [
            {
              "type": "boolean",
              "name": "hasBasement",
              "visibleIf": "{homeInsurance} = true",
              "title": "Is there a basement?",
              "formattedTitle": "Basement",
              "isRequired": true
            },
            {
              "type": "boolean",
              "name": "basementFinished",
              "visibleIf": "{hasBasement} = true",
              "startWithNewLine": false,
              "title": "Is the basement finished?",
              "formattedTitle": "Finished basement",
              "isRequired": true
            }
          ]
        },
        {
          "type": "panel",
          "name": "deckInfo",
          "title": "Deck Information",
          "visibleIf": "{homeInsurance} = true",
          "elements": [
            {
              "type": "boolean",
              "name": "hasDeck",
              "visibleIf": "{homeInsurance} = true",
              "title": "Is there a deck?",
              "formattedTitle": "Deck",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "deckSize",
              "visibleIf": "{hasDeck} = true",
              "title": "What is the size?",
              "formattedTitle": "Deck size"
            },
            {
              "type": "text",
              "name": "deckMaterial",
              "visibleIf": "{hasDeck} = true",
              "startWithNewLine": false,
              "title": "What is the material?",
              "formattedTitle": "Deck material"
            }
          ]
        },
        {
          "type": "panel",
          "name": "dogInfo",
          "title": "Pet Information",
          "visibleIf": "{homeInsurance} = true",
          "elements": [
            {
              "type": "boolean",
              "name": "hasDog",
              "visibleIf": "{homeInsurance} = true",
              "title": "Do you own any dogs?",
              "formattedTitle": "Dogs"
            },
            {
              "type": "text",
              "name": "dogBreed",
              "visibleIf": "{hasDog} = true",
              "startWithNewLine": false,
              "title": "Please list the breeds.",
              "formattedTitle": "Breeds",
              "size": 0
            }
          ]
        },
        {
          "type": "panel",
          "name": "hvacInfo",
          "title": "HVAC Information",
          "visibleIf": "{homeInsurance} = true",
          "elements": [
            {
              "type": "boolean",
              "name": "hvacRoof",
              "visibleIf": "{homeInsurance} = true",
              "title": "Has the HVAC or Roof been replaced?",
              "formattedTitle": "Replaced HVAC or roof",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "hvacRoofReplaceYear",
              "visibleIf": "{hvacRoof} = true",
              "startWithNewLine": false,
              "title": "What year?",
              "formattedTitle": "Year replaced",
              "inputType": "month"
            }
          ]
        },
        {
          "type": "boolean",
          "name": "hasPoolOrTrampoline",
          "visibleIf": "{homeInsurance} = true",
          "title": "Do you have a trampoline or pool?",
          "formattedTitle": "Trampoline or Pool",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "specialItemInfo",
          "visibleIf": "{homeInsurance} = true",
          "title": "Special Property Information",
          "elements": [
            {
              "type": "boolean",
              "name": "hasSpecialItems",
              "visibleIf": "{homeInsurance} = true",
              "title": "Do you have any special property you want specifically listed on the policy? (Ex: Jewelry, firearms, fine arts)",
              "formattedTitle": "Special property"
            },
            {
              "type": "comment",
              "name": "specialItems",
              "visibleIf": "{hasSpecialItems} = true",
              "startWithNewLine": false,
              "title": "Please list the items.",
              "formattedTitle": "Special property items",
              "rows": 2,
              "autoGrow": true
            }
          ]
        },
        {
          "type": "text",
          "name": "phone",
          "visibleIf": "{homeInsurance} = true",
          "title": "Best contact number?",
          "formattedTitle": "Contact Number",
          "validators": [
            {
              "type": "regex",
              "text": "Phone number must be in the following format: (123) 456-7890 or 123-456-7890",
              "regex": "^\\(?\\d{3}\\)?[-\\s.]?\\d{3}[-\\s.]?\\d{4}$"
            }
          ],
          "inputType": "tel",
          "autocomplete": "tel",
          "placeholder": "(123) 456-7890"
        }        
      ]
    },
    {
      "name": "autoInsurancePage1",
      "elements": [
        {
          "type": "boolean",
          "name": "autoInsurance",
          "title": "Interested in Auto insurance?",
          "formattedTitle": "Interested in Auto insurance?",
          "isRequired": true
        },
        {
          "type": "paneldynamic",
          "name": "automobileOwnerInfo",
          "visibleIf": "{autoInsurance} = true",
          "title": "Automobile owner information",
          "formattedTitle": "Automobile owner",
          "templateElements": [
            {
              "type": "text",
              "name": "firstNameAutomobile",
              "title": "First Name"
            },
            {
              "type": "text",
              "name": "lastNameAutomobile",
              "startWithNewLine": false,
              "title": "Last Name"
            },
            {
              "type": "text",
              "name": "birthDateAutomobile",
              "startWithNewLine": false,
              "title": "Date of Birth",
              "inputType": "date"
            },
            {
              "type": "text",
              "name": "licenseNumberAutomobile",
              "title": "Driver license number"
            },
            {
              "type": "text",
              "name": "stateAutomobile",
              "startWithNewLine": false,
              "title": "State"
            },
            {
              "type": "text",
              "name": "phoneAutomobile",
              "title": "Phone number",
              "validators": [
                {
                  "type": "regex",
                  "text": "Phone number must be in the following format: (123) 456-7890 or 123-456-7890",
                  "regex": "^\\(?\\d{3}\\)?[-\\s.]?\\d{3}[-\\s.]?\\d{4}$"
                }
              ],
              "inputType": "tel",
              "autocomplete": "tel",
              "placeholder": "(123) 456-7890"
            },
            {
              "type": "text",
              "name": "employerJobTitles",
              "startWithNewLine": false,
              "title": "Employer/Job Title (Possible discounts we can find)"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 20,
          "confirmDelete": true
        },
        {
          "type": "panel",
          "name": "panel1",
          "visibleIf": "{homeInsurance} = false and {autoInsurance} = true",
          "title": "Address",
          "isRequired": true,
          "elements": [
            {
              "type": "text",
              "name": "addressLineAutomobile1",
              "title": "Address",
              "isRequired": true,
              "placeholder": "495 Grove Street"
            },
            {
              "type": "text",
              "name": "addressLineAutomobile2",
              "title": "Address line 2",
              "placeholder": "Apartment #20"
            },
            {
              "type": "text",
              "name": "cityAutomobile",
              "title": "City/Town",
              "isRequired": true,
              "placeholder": "New York"
            },
            {
              "type": "text",
              "name": "stateAddressAutomobile",
              "startWithNewLine": false,
              "title": "State/Region/Province",
              "isRequired": true,
              "placeholder": "NY"
            },
            {
              "type": "text",
              "name": "postalAutomobile",
              "title": "Zip/Postal code",
              "isRequired": true,
              "inputType": "number",
              "min": 0,
              "max": 99999,
              "placeholder": "10014"
            },
            {
              "type": "dropdown",
              "name": "countryAutomobile",
              "title": "Country",
              "choicesByUrl": {
                "url": "https://surveyjs.io/api/CountriesExample"
              },
              "placeholder": "United States"
            }
          ]
        },
        {
          "type": "paneldynamic",
          "name": "vinInfo",
          "visibleIf": "{autoInsurance} = true",
          "title": "VIN's for all vehicles",
          "formattedTitle": "VIN for vehicle",
          "templateElements": [
            {
              "type": "text",
              "name": "vin",
              "title": "Vehicle identification number:"
            },
            {
              "type": "text",
              "name": "makeAuto",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "modelAuto",
              "startWithNewLine": false,
              "title": "Model:"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 20,
          "confirmDelete": true
        },
        {
          "type": "boolean",
          "name": "uberDelivery",
          "visibleIf": "{autoInsurance} = true",
          "title": "Do you do any Uber/Doordash or other delivery services with your vehicles?",
          "formattedTitle": "Uber/Doordash or other delivery services",
          "isRequired": true
        },
        {
          "type": "boolean",
          "name": "telematicsProgram",
          "visibleIf": "{autoInsurance} = true",
          "title": "Do you want to enroll in the telematics program? (where they track your driving) if available (usually about 10% off insurance rates)",
          "formattedTitle": "Enroll in the telematics program"
        },
        {
          "type": "boolean",
          "name": "collisionCoverage",
          "visibleIf": "{autoInsurance} = true",
          "startWithNewLine": false,
          "title": "Do you want Comprehensive/Collision coverage on your vehicles?",
          "formattedTitle": "Comprehensive/Collision coverage on their vehicles"
        }
      ]
    },
    {
      "name": "boatInsurancePage1",
      "elements": [
        {
          "type": "boolean",
          "name": "boatInsurance",
          "title": "Interested in boat insurance?",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "boatInfo",
          "visibleIf": "{boatInsurance} = true",
          "elements": [
            {
              "type": "text",
              "name": "hullNumber",
              "title": "Hull number:"
            },
            {
              "type": "text",
              "name": "makeBoat",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "modelBoat",
              "startWithNewLine": false,
              "title": "Model:"
            },
            {
              "type": "text",
              "name": "boatYear",
              "title": "Year:"
            },
            {
              "type": "text",
              "name": "lengthBoat",
              "startWithNewLine": false,
              "title": "Length:"
            },
            {
              "type": "text",
              "name": "motors",
              "title": "Number of motors:",
              "inputType": "number"
            },
            {
              "type": "text",
              "name": "horsepower",
              "startWithNewLine": false,
              "title": "Horsepower:"
            },
            {
              "type": "text",
              "name": "hullMaterial",
              "title": "Hull material:"
            },
            {
              "type": "text",
              "name": "boatStorageLocation",
              "title": "Where is the boat stored?"
            }
          ]
        },
        {
          "type": "boolean",
          "name": "boatTrailerInsurance",
          "visibleIf": "{boatInsurance} = true",
          "title": "Do you want to insure a trailer with the boat? ",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "trailerInfo",
          "visibleIf": "{boatTrailerInsurance} = true and {boatInsurance} = true",
          "title": "Trailer information",
          "elements": [
            {
              "type": "text",
              "name": "trailerID",
              "title": "Trailer ID:"
            },
            {
              "type": "text",
              "name": "trailerMake",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "trailerModel",
              "startWithNewLine": false,
              "title": "Model:"
            },
            {
              "type": "text",
              "name": "trailerYear",
              "title": "Year:"
            },
            {
              "type": "text",
              "name": "trailerValue",
              "startWithNewLine": false,
              "title": "Approximate value of trailer (in USD):"
            }
          ]
        }
      ]
    },
    {
      "name": "page3",
      "elements": [
        {
          "type": "boolean",
          "name": "rvInsurance",
          "title": "Interested in RV insurance?",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "rvInfo",
          "visibleIf": "{rvInsurance} = true",
          "title": "RV information",
          "elements": [
            {
              "type": "text",
              "name": "vinRv",
              "title": "VIN number:"
            },
            {
              "type": "text",
              "name": "yearRv",
              "startWithNewLine": false,
              "title": "Year:"
            },
            {
              "type": "text",
              "name": "makeRv",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "modelRv",
              "startWithNewLine": false,
              "title": "Model:"
            },
            {
              "type": "text",
              "name": "lengthRv",
              "title": "Length of vehicle:"
            },
            {
              "type": "boolean",
              "name": "hasBumpOuts",
              "title": "Are there any Bump outs?"
            },
            {
              "type": "text",
              "name": "daysOnRoad",
              "title": "How many days a year on the road?",
              "inputType": "number",
              "min": 1,
              "max": 365,
              "step": 1
            },
            {
              "type": "text",
              "name": "rvStorageLocation",
              "title": "Where is it typically stored?"
            }
          ]
        }
      ]
    },
    {
      "name": "page4",
      "elements": [
        {
          "type": "panel",
          "name": "contactInformation",
          "title": "Contact Information",
          "elements": [
            {
              "type": "text",
              "name": "email",
              "title": "Please include your email address",
              "formattedTitle": "Email Address",
              "description": "Your email address will not be used for marketing purposes.",
              "isRequired": false
            },
            {
              "type": "text",
              "name": "teamMember",
              "title": "Have you been working with a specific team member?",
              "description": "If so, please list their name to help us direct your request to the right agent.",
              "isRequired": false
            },
            {
              "type": "comment",
              "name": "additionalComments",
              "title": "Please include any questions, clarifying information, or comments",
              "formattedTitle": "Questions, clarifying information, or comments",
              "isRequired": false
            }
          ]
        }
        
      ]
    }
  ],
  "showQuestionNumbers": "off",
  "showProgressBar": "auto",
  "allowCompleteSurveyAutomatic": false
}