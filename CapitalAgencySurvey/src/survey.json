{
  "pages": [
    {
      "name": "homeInsurancePagel",
      "elements": [
        {
          "type": "boolean",
          "name": "homeInsurance",
          "title": "Interested in Home insurance?",
          "defaultValue": "true",
          "isRequired": true
        },
        {
          "type": "paneldynamic",
          "name": "homeOwnerInfo",
          "visibleIf": "{homeInsurance} = true",
          "title": "Owner Information (include spouses)",
          "templateElements": [
            {
              "type": "text",
              "name": "firstName",
              "title": "First Name",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "lastName",
              "startWithNewLine": false,
              "title": "Last Name",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "birthDate",
              "startWithNewLine": false,
              "title": "Date of birth",
              "isRequired": true,
              "inputType": "date",
              "min": "1900-01-01"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 5,
          "confirmDelete": true,
          "confirmDeleteText": "Are you sure you want to delete this owner?",
          "panelAddText": "Add owner",
          "panelRemoveText": "Remove owner"
        },
        {
          "type": "panel",
          "name": "homeOwnerAddress",
          "visibleIf": "{homeInsurance} = true",
          "title": "Address of Home",
          "isRequired": true,
          "elements": [
            {
              "type": "text",
              "name": "addressLine1",
              "title": "Address",
              "isRequired": true,
              "placeholder": "495 Grove Street"
            },
            {
              "type": "text",
              "name": "addressLine2",
              "title": "Address line 2",
              "placeholder": "Apartment #20"
            },
            {
              "type": "text",
              "name": "city",
              "title": "City/Town",
              "isRequired": true,
              "placeholder": "New York"
            },
            {
              "type": "text",
              "name": "state",
              "startWithNewLine": false,
              "title": "State/Region/Province",
              "isRequired": true,
              "placeholder": "NY"
            },
            {
              "type": "text",
              "name": "postCode",
              "title": "Zip/Postal code",
              "isRequired": true,
              "inputType": "number",
              "min": 501,
              "max": 14925,
              "placeholder": "10014"
            },
            {
              "type": "dropdown",
              "name": "country",
              "title": "Country",
              "choicesByUrl": {
                "url": "https://surveyjs.io/api/CountriesExample"
              },
              "placeholder": "USA"
            },
            {
              "type": "boolean",
              "name": "isCurrentAddress",
              "title": "This is my current address.",
              "isRequired": true
            }
          ]
        },
        {
          "type": "panel",
          "name": "currentAddress",
          "visibleIf": "{homeInsurance} = true and {isCurrentAddress} = false",
          "title": "Current Address",
          "elements": [
            {
              "type": "text",
              "name": "currentAddressLine1",
              "title": "Address",
              "isRequired": true,
              "placeholder": "495 Grove Street"
            },
            {
              "type": "text",
              "name": "currentAddressLine2",
              "title": "Address line 2",
              "placeholder": "Apartment #20"
            },
            {
              "type": "text",
              "name": "currentCity",
              "title": "City/Town",
              "isRequired": true,
              "placeholder": "New York"
            },
            {
              "type": "text",
              "name": "currentState",
              "startWithNewLine": false,
              "title": "State/Region/Province",
              "isRequired": true,
              "placeholder": "NY"
            },
            {
              "type": "text",
              "name": "currentPostCode",
              "title": "Zip/Postal code",
              "isRequired": true,
              "inputType": "number",
              "min": 501,
              "max": 14925,
              "placeholder": "10014"
            },
            {
              "type": "dropdown",
              "name": "currentCountry",
              "title": "Country",
              "choicesByUrl": {
                "url": "https://surveyjs.io/api/CountriesExample"
              },
              "placeholder": "USA"
            }
          ]
        },
        {
          "type": "panel",
          "name": "basementInfo",
          "elements": [
            {
              "type": "boolean",
              "name": "hasBasement",
              "visibleIf": "{homeInsurance} = true",
              "title": "Is there a basement?",
              "isRequired": true
            },
            {
              "type": "boolean",
              "name": "basementFinished",
              "visibleIf": "{hasBasement} = true",
              "startWithNewLine": false,
              "title": "Is the basement finished?",
              "isRequired": true
            }
          ]
        },
        {
          "type": "panel",
          "name": "deckInfo",
          "elements": [
            {
              "type": "boolean",
              "name": "hasDeck",
              "visibleIf": "{homeInsurance} = true",
              "title": "Is there a deck?",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "deckSize",
              "visibleIf": "{hasDeck} = true",
              "title": "What is the size?"
            },
            {
              "type": "text",
              "name": "deckMaterial",
              "visibleIf": "{hasDeck} = true",
              "startWithNewLine": false,
              "title": "What is the material?"
            }
          ]
        },
        {
          "type": "panel",
          "name": "dogInfo",
          "elements": [
            {
              "type": "boolean",
              "name": "hasDog",
              "visibleIf": "{homeInsurance} = true",
              "title": "Do you own any dogs?"
            },
            {
              "type": "text",
              "name": "dogBreed",
              "visibleIf": "{hasDog} = true",
              "startWithNewLine": false,
              "title": "Please list the breeds.",
              "size": 0
            }
          ]
        },
        {
          "type": "panel",
          "name": "hvacInfo",
          "elements": [
            {
              "type": "boolean",
              "name": "hvacRoof",
              "visibleIf": "{homeInsurance} = true",
              "title": "Has the HVAC or Roof been replaced?",
              "isRequired": true
            },
            {
              "type": "text",
              "name": "hvacRoofReplaceYear",
              "visibleIf": "{hvacRoof} = true",
              "startWithNewLine": false,
              "title": "What year?",
              "inputType": "month"
            }
          ]
        },
        {
          "type": "boolean",
          "name": "question1",
          "visibleIf": "{homeInsurance} = true",
          "title": "Do you have a trampoline or pool?",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "specialItemInfo",
          "elements": [
            {
              "type": "boolean",
              "name": "hasSpecialItems",
              "visibleIf": "{homeInsurance} = true",
              "title": "Do you have any special property you want specifically listed on the policy? (Ex: Jewelry, firearms, fine arts)"
            },
            {
              "type": "comment",
              "name": "specialItems",
              "visibleIf": "{hasSpecialItems} = true",
              "startWithNewLine": false,
              "title": "Please list the items.",
              "rows": 2,
              "autoGrow": true
            }
          ]
        },
        {
          "type": "text",
          "name": "phone",
          "visibleIf": "{homeInsurance} = true",
          "title": "Best contact number?",
          "validators": [
            {
              "type": "regex",
              "text": "Phone number must be in the following format: +0 (000) 000-00-00",
              "regex": "^\\+?[0-9\\s\\-\\(\\)]{7,20}$"
            }
          ],
          "inputType": "tel",
          "autocomplete": "tel",
          "placeholder": "+0 (000) 000-00-00"
        }
      ]
    },
    {
      "name": "autoInsurancePage1",
      "elements": [
        {
          "type": "boolean",
          "name": "autoInsurance",
          "title": "Interested in Auto insurance?",
          "isRequired": true
        },
        {
          "type": "paneldynamic",
          "name": "automobileOwnerInfo",
          "visibleIf": "{autoInsurance} = true",
          "title": "Automobile owner information",
          "templateElements": [
            {
              "type": "text",
              "name": "firstNameAutomobile",
              "title": "First Name"
            },
            {
              "type": "text",
              "name": "lastNameAutomobile",
              "startWithNewLine": false,
              "title": "Last Name"
            },
            {
              "type": "text",
              "name": "birthDateAutomobile",
              "startWithNewLine": false,
              "title": "Date of Birth",
              "inputType": "date"
            },
            {
              "type": "text",
              "name": "licenseNumberAutomobile",
              "title": "Driver license number",
              "inputType": "number"
            },
            {
              "type": "text",
              "name": "stateAutomobile",
              "startWithNewLine": false,
              "title": "State"
            },
            {
              "type": "text",
              "name": "phoneAutomobile",
              "title": "Phone number",
              "validators": [
                {
                  "type": "regex",
                  "text": "Phone number must be in the following format: +0 (000) 000-00-00",
                  "regex": "^\\+?[0-9\\s\\-\\(\\)]{7,20}$"
                }
              ],
              "inputType": "tel",
              "autocomplete": "tel",
              "placeholder": "+0 (000) 000-00-00"
            },
            {
              "type": "text",
              "name": "employerJobTitles",
              "startWithNewLine": false,
              "title": "Employer/Job Title (Possible discounts we can find)"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 20,
          "confirmDelete": true
        },
        {
          "type": "panel",
          "name": "panel1",
          "visibleIf": "{homeInsurance} = false and {autoInsurance} = true",
          "title": "Address",
          "isRequired": true,
          "elements": [
            {
              "type": "text",
              "name": "addressLineAutomobile1",
              "title": "Address",
              "isRequired": true,
              "placeholder": "495 Grove Street"
            },
            {
              "type": "text",
              "name": "addressLineAutomobile2",
              "title": "Address line 2",
              "placeholder": "Apartment #20"
            },
            {
              "type": "text",
              "name": "cityAutomobile",
              "title": "City/Town",
              "isRequired": true,
              "placeholder": "New York"
            },
            {
              "type": "text",
              "name": "stateAddressAutomobile",
              "startWithNewLine": false,
              "title": "State/Region/Province",
              "isRequired": true,
              "placeholder": "NY"
            },
            {
              "type": "text",
              "name": "postalAutomobile",
              "title": "Zip/Postal code",
              "isRequired": true,
              "inputType": "number",
              "min": 501,
              "max": 14925,
              "placeholder": "10014"
            },
            {
              "type": "dropdown",
              "name": "countryAutomobile",
              "title": "Country",
              "choicesByUrl": {
                "url": "https://surveyjs.io/api/CountriesExample"
              },
              "placeholder": "USA"
            }
          ]
        },
        {
          "type": "paneldynamic",
          "name": "vinInfo",
          "visibleIf": "{autoInsurance} = true",
          "title": "VIN's for all vehicles",
          "templateElements": [
            {
              "type": "text",
              "name": "vin",
              "title": " Vehicle identification number:"
            }
          ],
          "panelCount": 1,
          "minPanelCount": 1,
          "maxPanelCount": 20,
          "confirmDelete": true
        },
        {
          "type": "boolean",
          "name": "uberDelivery",
          "visibleIf": "{autoInsurance} = true",
          "title": "Do you do any Uber/Doordash or other delivery services with your vehicles?",
          "isRequired": true
        },
        {
          "type": "boolean",
          "name": "telematicsProgram",
          "visibleIf": "{autoInsurance} = true",
          "title": "Do you want to enroll in the telematics program? (where they track your driving) if available (usually about 10% off insurance rates)"
        },
        {
          "type": "boolean",
          "name": "collisionCoverage",
          "visibleIf": "{autoInsurance} = true",
          "startWithNewLine": false,
          "title": "Do you want Comprehensive/Collision coverage on your vehicles?"
        }
      ]
    },
    {
      "name": "boatInsurancePage1",
      "elements": [
        {
          "type": "boolean",
          "name": "boatInsurance",
          "title": "Interested in boat insurance?",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "boatInfo",
          "visibleIf": "{boatInsurance} = true",
          "elements": [
            {
              "type": "text",
              "name": "hullNumber",
              "title": "Hull number:",
              "inputType": "number"
            },
            {
              "type": "text",
              "name": "makeBoat",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "modelBoat",
              "startWithNewLine": false,
              "title": "Model:"
            },
            {
              "type": "text",
              "name": "boatYear",
              "title": "Year:",
              "inputType": "month"
            },
            {
              "type": "text",
              "name": "lengthBoat",
              "startWithNewLine": false,
              "title": "Length:"
            },
            {
              "type": "text",
              "name": "motors",
              "title": "Number of motors:",
              "inputType": "number"
            },
            {
              "type": "text",
              "name": "horsepower",
              "startWithNewLine": false,
              "title": "Horsepower:"
            },
            {
              "type": "text",
              "name": "hullMaterial",
              "title": "Hull material:"
            },
            {
              "type": "text",
              "name": "boatStorageLocation",
              "title": "Where is the boat stored?"
            }
          ]
        },
        {
          "type": "boolean",
          "name": "boatTrailerInsurance",
          "visibleIf": "{boatInsurance} = true",
          "title": "Do you want to insure a trailer with the boat? ",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "trailerInfo",
          "visibleIf": "{boatTrailerInsurance} = true",
          "title": "Trailer information",
          "elements": [
            {
              "type": "text",
              "name": "trailerID",
              "title": "Trailer ID:"
            },
            {
              "type": "text",
              "name": "trailerMake",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "trailerModel",
              "startWithNewLine": false,
              "title": "Model:"
            },
            {
              "type": "text",
              "name": "trailerYear",
              "title": "Year:",
              "inputType": "month"
            },
            {
              "type": "text",
              "name": "trailerValue",
              "startWithNewLine": false,
              "title": "Approximate value of trailer:",
              "inputType": "number"
            }
          ]
        }
      ]
    },
    {
      "name": "page3",
      "elements": [
        {
          "type": "boolean",
          "name": "rvInsurance",
          "title": "Interested in RV insurance?",
          "isRequired": true
        },
        {
          "type": "panel",
          "name": "rvInfo",
          "visibleIf": "{rvInsurance} = true",
          "title": "RV information",
          "elements": [
            {
              "type": "text",
              "name": "vinRv",
              "title": "VIN number:",
              "isRequired": true,
              "inputType": "number"
            },
            {
              "type": "text",
              "name": "yearRv",
              "startWithNewLine": false,
              "title": "Year:"
            },
            {
              "type": "text",
              "name": "makeRv",
              "title": "Make:"
            },
            {
              "type": "text",
              "name": "modelRv",
              "startWithNewLine": false,
              "title": "Model:"
            },
            {
              "type": "text",
              "name": "lengthRv",
              "title": "Length of vehicle:"
            },
            {
              "type": "boolean",
              "name": "hasBumpOuts",
              "title": "Are there any Bump outs?"
            },
            {
              "type": "text",
              "name": "daysOnRoad",
              "title": "How many days a year on the road?",
              "inputType": "number",
              "min": 1,
              "max": 365,
              "step": 1
            },
            {
              "type": "text",
              "name": "rvStorageLocation",
              "title": "Where is it typically stored?"
            }
          ]
        }
      ]
    }
  ],
  "showQuestionNumbers": "off",
  "showProgressBar": "auto",
  "allowCompleteSurveyAutomatic": false
}